<Window x:Class="Gui_Programmering.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gui_Programmering"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Grid>
        <Button Content="Start" HorizontalAlignment="Left" Height="30" Margin="100,220,0,0" VerticalAlignment="Top" Width="100" x:Name="btnStart" Click="btnStart_Click"/>
        <TextBox HorizontalAlignment="Left" Height="20" Margin="100,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Name="Player1"/>
        <TextBox HorizontalAlignment="Left" Height="20" Margin="100,120,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Name="player2"/>
        <Label Content="Spiller 2:" HorizontalAlignment="Left" Height="30" Margin="40,115,0,0" VerticalAlignment="Top" Width="60"/>
        <Label Content="Spiller 1:" HorizontalAlignment="Left" Height="30" Margin="40,75,0,0" VerticalAlignment="Top" Width="60"/>
    </Grid>
</Window>
